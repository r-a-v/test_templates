import DITranquillity

extension DIHelper {

  func create{{ module_info.name }}View() -> {{ module_info.name }}View {
      let view: {{ module_info.name }}View = self.container.resolve()
      let presenter: {{ module_info.name }}Presenter = self.container.resolve()
      let router: {{ module_info.name }}Router = self.container.resolve()
      let useCases: {{ module_info.name }}UseCases = self.container.resolve()

      view.injectPresenter(presenter)

      presenter.injectView(view)
          .injectRouter(router)
          .injectUseCases(useCases)

      router.injectView(view)

      return view
  }
}