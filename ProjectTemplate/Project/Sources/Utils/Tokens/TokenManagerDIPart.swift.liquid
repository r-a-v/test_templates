import DITranquillity
import Foundation

final class TokenManagerDIPart: DIPart {

    static func load(container: DIContainer) {
        container.register(TokenManagerImpl.init)
            .as(TokenManager.self)
            .lifetime(.single)
    }
}

extension DIManagers {

    @objc
    func loadManagerTokenDIPart() {
        self.parts.append(TokenManagerDIPart.self)
    }

}