import Foundation
import SQDefaults
import Entities

struct Defaults {

    @StoredValue(key: .currentServer, defaultValue: "")
    static var currentServer: String!

    @StoredValue(key: .customServer, defaultValue: nil)
    static var customServer: String?
}

private extension String {

    static let customServer  = "customServer"
    static let currentServer = "currentServer"
}
