import UIKit
import SQVUPER

// MARK: - BaseRouter
protocol BaseRouter: Router {

    @discardableResult
    func injectView(_ view: BaseView) -> Self 
}

// MARK: - BaseRouterImpl
class BaseRouterImpl: BaseRouter {

// MARK: - VUPER
    internal weak var _internalView: BaseView?

// MARK: - DI
    @discardableResult
    func injectView(_ view: BaseView) -> Self {
        self._internalView = view
        return self
    }

}