import RealmSwift
import SQRealm

final class RealmRepository<T>: SQRealm.RealmRepository where T: RealmEntity {

    typealias DBEntityType = T

    var realm: Realm?

    convenience init() {
        self.init(realm: RealmHelper.instance.realm)
    }

    required init(realm: Realm?) {
        self.realm = realm
    }

}
